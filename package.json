{
  "name": "@dashfy/default-ui",
  "private": true,
  "description": "The default template for new Dashfy appplications.",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H ${UI_HOST} -p ${UI_PORT}",
    "local": "gatsby develop",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write ./src",
    "start": "npm run local",
    "serve": "gatsby serve -H ${UI_HOST} -p ${UI_PORT}",
    "clean": "gatsby clean",
    "flow": "flow",
    "test": "jest --passWithNoTests --testPathPattern=src",
    "test:watch": "jest --watch --testPathPattern=src --passWithNoTests"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "classnames": "^2.2.6",
    "formik": "^2.2.0",
    "gatsby": "^2.24.80",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-flow": "^1.3.13",
    "gatsby-plugin-manifest": "^2.4.35",
    "gatsby-plugin-offline": "^3.2.33",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sass": "^2.3.17",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-source-filesystem": "^2.3.35",
    "gatsby-source-strapi": "0.0.12",
    "gatsby-transformer-sharp": "^2.5.18",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "nouislider": "^14.6.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap-switch": "^15.5.3",
    "react-datetime": "^2.16.3",
    "react-dom": "^16.13.1",
    "react-google-maps": "^9.4.5",
    "react-helmet": "^6.1.0",
    "react-player": "^2.6.2",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "reactstrap": "^8.6.0",
    "recompose": "^0.30.0",
    "styled-components": "^5.2.0",
    "gatsby-alias-imports": "^1.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^0.5.16",
    "eslint": "6.6.0",
    "eslint-config-flowtype-essential": "^1.0.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "flow-bin": "^0.137.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.5.1",
    "prettier": "2.1.2",
    "prettier-eslint-cli": "^5.0.0",
    "react-test-renderer": "^16.13.1"
  },
  "keywords": [
    "dashfy"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/layoutzweb/dashfy-frontend"
  },
  "bugs": {
    "url": "https://github.com/layoutzweb/dashfy-frontend/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*./src": "prettier --write"
  }
}
